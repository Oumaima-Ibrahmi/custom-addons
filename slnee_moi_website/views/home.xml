    <template id="layout_inherit" name="Layout - Inherit" inherit_id="website.layout">
        <xpath expr="//header" position="replace"></xpath>
        <xpath expr="//footer" position="replace">
            <footer t-if="not no_footer" id="bottom" data-anchor="true" class="">
                <div class="o_footer_copyright">
                    <div class="container py-3">
                        <div class="row">
                            <div class="col-sm text-center text-white">
                                <span>All rights reserved to the Ministry of Interior Development Program © 2023- Saudi
                                    Arabia
                                </span>
                            </div>
                        </div>
                    </div>
                </div>
            </footer>
        </xpath>
    </template>

    <template id="page_login" name="SLNEE - Login Page" inherit_id="website.login_layout">
        <xpath expr="t" position="replace">
            <t t-call="website.layout">
                <div class="main-div">
                    <div class="background_image"/>
                    <div class="inline-logo">
                        <img src="/slnee_moi_website/static/src/img/inline_logo.png" alt="Logo"/>
                    </div>
                    <div class="content bg-custom-white" style="height: 64vh;">
                        <div class="container">
                            <nav class="navbar navbar-expand-md">
                                <ul class="nav navbar-nav">
                                    <li class="nav-item">
                                        <a t-attf-class="nav-link text-dark-green"
                                           href="/my-home">Home
                                        </a>
                                    </li>
                                    <li class="nav-item">
                                        <a t-attf-class="nav-link text-dark-green"
                                           href="/my-home/service/login">Service Desk
                                        </a>
                                    </li>
                                    <li class="nav-item">
                                        <a t-attf-class="nav-link text-dark-green active"
                                           href="/my-home/erp/login">ERP System
                                        </a>
                                    </li>
                                    <li class="nav-item">
                                        <a t-attf-class="nav-link text-dark-green"
                                           href="/my-home/communication/login">Communication
                                        </a>
                                    </li>
                                    <li class="nav-item">
                                        <a t-attf-class="nav-link text-dark-green"
                                           href="/my-home/reservation/login">Hall Reservation
                                        </a>
                                    </li>
                                    <li class="nav-item">
                                        <a t-attf-class="nav-link text-dark-green"
                                           href="/my-home/management/login">Task Management
                                        </a>
                                    </li>
                                </ul>
                            </nav>
                            <div class="page-title">
                                <div class="row d-flex">
                                    <span>
                                        <i class="fas fa-house-user"></i>
                                    </span>
                                </div>
                            </div>
                            <div class="oe_website_login_container" t-raw="0"/>
                        </div>
                    </div>
                </div>
            </t>
        </xpath>
    </template>

    <template id="dashboard_options" name="SLNEE - Dashboard Options">
        <t t-if="leaves">
            <div class="card bg-c-lite-green option">
                <div class="card-block">
                    <div class="row align-items-end">
                        <div class="col-12">
                            <h4 class="text-white"><t t-esc="len(leaves.ids)"/> طلب</h4>
                            <h6 class="text-white m-b-0">إجازات</h6>
                        </div>
                    </div>
                </div>
                <div class="card-footer" style="border-top: 1px solid rgba(255, 255, 255, 0.125)">
                    <p class="text-white m-b-0"><i class="feather icon-clock text-white f-14 m-l-10"></i>آخر تحديث
                         : 10 ديسمبر 2023</p>
                </div>
            </div>
        </t>
    </template>

